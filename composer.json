{
	"name": "jheaksoft/jhekasoft3",
	"description": "Jhekasoft website",
	"keywords": ["jhekasoft", "yii"],
	"homepage": "http://jhekasoft.net/",
	"type": "project",
	"license": "BSD-3-Clause",
	"support": {
		"source": "https://github.com/jhekasoft/jhekasoft3"
	},
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.4.0",
		"yiisoft/yii2": "*",
		"yiisoft/yii2-bootstrap": "*",
		"yiisoft/yii2-swiftmailer": "*"
	},
	"require-dev": {
		"yiisoft/yii2-codeception": "*",
		"codeception/codeception": "*",
		"codeception/specify": "*",
		"codeception/verify": "*",
		"yiisoft/yii2-debug": "*",
		"yiisoft/yii2-gii": "*"
	},
	"scripts": {
		"post-create-project-cmd": [
			"yii\\composer\\Installer::setPermission"
		]
	},
	"config": {
		"process-timeout": 1800
	},
	"extra": {
		"writable": [
			"runtime",
			"web/assets"
		],
		"executable": [
			"yii"
		]
	}
}
